<div id="container">
  <div id="itemList">
    <div id="itemListToolbar">
      <div id="links">
        <a href="http://news.ycombinator.com" id="top" class="active">Top</a>
        <a href="http://news.ycombinator.com/newest" id="newest">New</a>
        <span class="right">
          <a id="refresh" href="#refresh" title="Refresh">
            <img id="refresh-icon" src="/img/01-refresh.png" alt="Refresh" />
          </a>
        </span>
      </div>
    </div>
    <div id="items" class="scrollbar"></div>
  </div>
  <div id="itemDetail">
    <iframe id="itemContent" src="/intro"></iframe>
  </div>
</div>
<div id="loadingIndicator" style="display:none;">Loading...</div>
<script id="item-template" type="text/x-handlebars-template">
  <div class="item">
    <h2><a href="{{link}}" target="_blank" class="go_to_site">{{title}}</a></h2>
    <div class="detail">
      <div class="right">
        <span class="site-link"><a href="{{link}}" target="_blank" title="Go to page" class="go_to_site"><img src="/img/113-navigation.png" height="15" /></a></span>
        <span class="comments"><a href="{{comments}}" title="View Comments"><img src="/img/08-chat.png" height="15" /></a></span>
      </div>
      <span class="site">{{host}}</span>
    </div>
  </div>
</script>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script>
<script>!window.jQuery && document.write(unescape('%3Cscript src="js/libs/jquery-1.6.1.min.js"%3E%3C/script%3E'))</script>
<%- scripts %>
